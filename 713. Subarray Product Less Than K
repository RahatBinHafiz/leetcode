class Solution {
public:
    #define ll long long int
    int numSubarrayProductLessThanK(vector<int>& nums, int k) {
        int n = nums.size();
        ll ans =0;
        ll x = 1;
        ll cnt = 1;
        ll start = 0;
        for(int i =0; i<n; i++)
        {
          x*=nums[i];
          if(x>=k)
          {
             
             while(cnt>0 && x >= k)
             {
                x/=nums[start];
                start++;
                cnt--;
             }

             
          }
          cout << cnt << endl;
          ans+=cnt;
          cnt++;
        }
        return ans;
    }
};
