class Solution {
public:
    #define ll long long int
    vector<bool> prefixesDivBy5(vector<int>& nums) {
        int n = nums.size();
        vector<bool>ans;
        ll sum = 0;
        for(auto & i:nums)
        {
            sum = ((sum << 1) + i) %5;
            ans.push_back(sum == 0);
        }
        return ans;
    }
};
