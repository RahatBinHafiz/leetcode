class Solution {
public:
    int maximizeSquareHoleArea(int n, int m, vector<int>& h, vector<int>& v) {

    sort(v.begin(), v.end());
    sort(h.begin(), h.end());
    int vmx = 1, hmx = 1, vcur= 1, hcur =1;
    for(int i = 1; i<v.size(); i++)
    {
       if(v[i] == v[i-1]+1)
       {
        vcur++;
       }
       else {
        vcur = 1;
       }
       vmx = max(vmx, vcur);
    }
    for(int i = 1; i<h.size(); i++)
    {
       if(h[i] == h[i-1]+1)
       {
        hcur++;
       }
       else {
        hcur = 1;
       }
       hmx = max(hmx, hcur);
    }
        int x = min(vmx,hmx)+1; 
        return x*x;
    }
};
