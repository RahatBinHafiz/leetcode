class Solution {
public:
    #define ll long long int 
    ll mod = 1e9 + 7;
    int numberOfWays(string corridor) {
         ll n = corridor.size();

         ll ans = 1;

         vector<ll>index;
         for(int i = 0; i<n; i++)
          {
            if(corridor[i] == 'S')
            index.push_back(i);
          }
          if(index.empty() || (index.size()&1)) return 0;

          int l = 1 , r = 2;
          while(r < index.size())
          {
             ans *= (index[r] - index[l] );
             ans %= mod;
             l+=2; r+=2;
          }
          return ans;

    }
};
