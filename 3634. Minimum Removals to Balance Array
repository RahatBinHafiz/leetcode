class Solution {
public:
    #define ll long long int
    int minRemoval(vector<int>& nums, int k) {
        int n = nums.size(); sort(nums.begin(), nums.end());
        ll ans = 0;
        ll l = 0;
        for(int r = 0; r<n; r++)
        {
          while(nums[r] > (ll)nums[l]*k)
          {
            l++;
          }
          ans = max(ans, r-l+1);

        }
        return n - ans;
    }
};
